journal = {Biological Psychology},
keywords = {Coping,Ecological momentary assessment,Emotion regulation,Heart rate variability,Polyvagal theory,Self-regulation,friese2},
mendeley-tags = {friese2},
pages = {279--286},
pmid = {23466587},
publisher = {Elsevier B.V.},
title = {{Cardiac vagal tone is associated with social engagement and self-regulation}},
