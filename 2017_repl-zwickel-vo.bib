journal = {Frontiers in neuroinformatics},
keywords = {EEG,MEG,Neuroscience,Psychophysics,Software,Vision,eeg,fMRI,fmri,meg,neuroscience,psychophysics,python,software,vision},
number = {January},
pages = {10},
