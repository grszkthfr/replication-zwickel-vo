pages = {212--226},
shorttitle = {Effort mobilization when the self is involved},
title = {{Effort mobilization when the self is involved: Some lessons from the cardiovascular system.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0019742},
volume = {14},
year = {2010}
}

