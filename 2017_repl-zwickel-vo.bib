keywords = {trait self control},
mendeley-tags = {trait self control},
pages = {76--99},
pmid = {21878607},
shorttitle = {Taking stock of self-control},
title = {{Taking stock of self-control: A meta-analysis of how trait self-control relates to a wide range of behaviors}},
url = {http://dare.ubvu.vu.nl/handle/1871/40957 http://psr.sagepub.com/content/16/1/76.full.pdf},
volume = {16},
