@article{Frischen2004,
abstract = {One component of successful social interactions is joint attention. It is now well established that when a gaze shift is observed, the observer's attention rapidly and automatically orients to the same location in space. It is also established that such attention shifts via gaze are relatively transient and do not evoke subsequent inhibition processes. In contrast to this conventional view, the authors conducted a series of studies that showed that these properties of gaze attention shift are not necessarily the case in all situations. The article demonstrates (a) gaze cuing over longer intervals than previously observed, (b) that these longer term effects can be inhibitory, and (c) that the longer term gaze cuing effects do not appear to be contingent on retrieval associated with a particular face identity.},
author = {Frischen, Alexandra and Tipper, Steven P.},
doi = {10.1037/0096-3445.133.4.516},
file = {:Frischen, Tipper_2004.pdf:pdf},
isbn = {0096-3445$\backslash$r1939-2222},
issn = {1939-2222},
journal = {Journal of Experimental Psychology: General},
number = {4},
pages = {516--533},
pmid = {15584804},
title = {{Orienting Attention Via Observed Gaze Shift Evokes Longer Term Inhibitory Effects: Implications for Social Interactions, Attention, and Memory.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/0096-3445.133.4.516},
volume = {133},
year = {2004}
}
