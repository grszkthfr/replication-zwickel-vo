isbn = {0306-4530},
issn = {03064530},
journal = {Psychoneuroendocrinology},
keywords = {1,Empathy,Inter-group dynamics,Intractable conflict,Jana,Oxytocin,Pain},
mendeley-tags = {1,Jana},
number = {12},
pages = {3139--3144},
pmid = {24099859},
publisher = {Elsevier Ltd},
title = {{Giving peace a chance: Oxytocin increases empathy to pain in the context of the Israeli-Palestinian conflict}},
