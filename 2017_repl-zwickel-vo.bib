title = {{Beliefs about willpower determine the impact of glucose on self-control.}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1313475110 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3773743{\&}tool=pmcentrez{\&}rendertype=abstract},
