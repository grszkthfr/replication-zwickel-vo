issn = {01651781},
journal = {Psychiatry Research},
keywords = {Exploratory excitability,Fatigability,Gender difference,Heart-rate variability,Personality},
pages = {652--657},
pmid = {23499230},
publisher = {Elsevier},
title = {{Gender differences in personality and heart-rate variability}},
url = {http://dx.doi.org/10.1016/j.psychres.2013.01.031},
volume = {209},
year = {2013}
}
