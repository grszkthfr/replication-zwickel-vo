journal = {NeuroImage},
keywords = {Adult,Autonomic Nervous System,Autonomic Nervous System: physiology,Brain,Brain Mapping,Brain: blood supply,Brain: physiology,Cerebrovascular Circulation,Cerebrovascular Circulation: physiology,Computer-Assisted,Electrocardiography,Emotions,Emotions: physiology,Female,Heart Rate,Heart Rate: physiology,Humans,Image Interpretation,Positron-Emission Tomography},
