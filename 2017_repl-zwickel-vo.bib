mendeley-tags = {E07},
number = {5},
pages = {656--667},
pmid = {18395185},
publisher = {Elsevier Ltd},
title = {{Effects of threat cues on attentional shifting, disengagement and response slowing in anxious individuals}},
url = {http://dx.doi.org/10.1016/j.brat.2008.02.011},
volume = {46},
year = {2008}
