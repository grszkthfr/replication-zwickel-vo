issn = {1531-5320},
journal = {Psychon Bull Rev},
keywords = {AuB08,anterior cingulate cortex,binding,cognitive control,conflict adaptation,conflict monitoring,contingency,contingency learning,feature-integration,gratton,information,ispc,proportion congruent,resolve conflict,response-conflict,stimulus-intensity,stroop,stroop task,temporal learning,traumatic brain-injury},
mendeley-tags = {AuB08},
number = {4},
