journal = {Psychological bulletin},
keywords = {AuB09,adaptive and flexible in,changing environmental demands,cognitive control,human behavior is highly,interference,not only,plex cognitive control processes,response to,task switching,this flexibility requires com-,which allow humans to},
mendeley-tags = {AuB09},
pages = {849--874},
