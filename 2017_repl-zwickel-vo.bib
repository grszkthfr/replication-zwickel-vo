@misc{Inzlicht2012,
abstract = {According to the resource model of self-control, overriding one's predominant response tendencies consumes and temporarily depletes a limited inner resource. Over 100 experiments have lent support to this model of ego depletion by observing that acts of self-control at Time 1 reduce performance on subsequent, seemingly unrelated self-control tasks at Time 2. The time is now ripe, therefore, not only to broaden the scope of the model but to start gaining a precise, mechanistic account of it. Accordingly, in the current article, the authors probe the particular cognitive, affective, and motivational mechanics of self-control and its depletion, asking, “What is ego depletion?” This study proposes a process model of depletion, suggesting that exerting self-control at Time 1 causes temporary shifts in both motivation and attention that undermine self-control at Time 2. The article highlights evidence in support of this model but also highlights where evidence is lacking, thus providing a blueprint for future research. Though the process model of depletion may sacrifice the elegance of the resource metaphor, it paints a more precise picture of ego depletion and suggests several nuanced predictions for future research},
author = {Inzlicht, Michael and Schmeichel, Brandon J.},
booktitle = {Perspectives on Psychological Science},
doi = {10.1177/1745691612454134},
file = {:Inzlicht, Schmeichel_2012.pdf:pdf},
isbn = {1745-6916},
issn = {1745-6916},
keywords = {Friese,self-control},
language = {en},
mendeley-tags = {Friese,self-control},
pages = {450--463},
shorttitle = {What Is Ego Depletion?},
title = {{What is ego depletion? Toward a mechanistic revision of the resource model of self-control}},
url = {http://pps.sagepub.com/lookup/doi/10.1177/1745691612454134},
volume = {7},
year = {2012}
}
@article{VanAst2016,
abstract = {Social threat can have adverse effects on cognitive performance, but the$\backslash$nbrain mechanisms underlying its effects are poorly understood. We$\backslash$ninvestigated the effects of social evaluative threat on working memory$\backslash$n(WM), a core component of many important cognitive capabilities. Social$\backslash$nthreat impaired WM performance during an N-back task and produced$\backslash$nwidespread reductions in activation in lateral prefrontal cortex and$\backslash$nintraparietal sulcus (IPS), among other regions. In addition, activity$\backslash$nin frontal and parietal regions predicted WM performance, and mediation$\backslash$nanalyses identified regions in the bilateral IPS that mediated the$\backslash$nperformance-impairing effects of social threat. Social threat also$\backslash$ndecreased connectivity between the IPS and dorsolateral prefrontal$\backslash$ncortex, while increasing connectivity between the IPS and the$\backslash$nventromedial prefrontal cortex, a region strongly implicated in the$\backslash$ngeneration of autonomic and emotional responses. Finally, cortisol$\backslash$nresponse to the stressor did not mediate WM impairment but was rather$\backslash$nassociated with protective effects. These results provide a basis for$\backslash$nunderstanding interactions between social and cognitive processes at a$\backslash$nneural systems level.},
author = {{Van Ast}, V. A. and Spicer, J. and Smith, E. E. and Schmer-Galunder, S. and Liberzon, I. and Abelson, J. L. and Wager, T. D.},
doi = {10.1093/cercor/bhu206},
file = {:Van Ast et al._2016.pdf:pdf},
isbn = {1047-3211},
issn = {14602199},
journal = {Cerebral Cortex},
keywords = {Executive function,FMRI,Intraparietal sulcus,Mediation,Stress},
number = {2},
pages = {544--556},
pmid = {25249408},
title = {{Brain Mechanisms of Social Threat Effects on Working Memory}},
volume = {26},
year = {2016}
}

