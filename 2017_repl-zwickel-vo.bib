journal = {Frontiers in human neuroscience},
keywords = {2,Sandy,beliefs,deception,fMR,fMRI,fmri,lie-detection,mock-crime},
mendeley-tags = {2,Sandy},
number = {February},
pages = {16},
pmid = {23382715},
publisher = {Frontiers Media SA},
title = {{When Pinocchio's nose does not grow: belief regarding lie-detectability modulates production of deception.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23382715 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3563087 http://journal.frontiersin.org/article/10.3389/fnhum.2013.00016/abstract},
