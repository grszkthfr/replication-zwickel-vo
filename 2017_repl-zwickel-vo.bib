language = {en},
month = {mar},
pages = {433--439},
pmid = {15741842},
shorttitle = {Effects of Exercise on Heart Rate Variability},
title = {{Effects of exercise on heart rate variability: inferences from meta-analysis}},
url = {http://content.wkhealth.com/linkback/openurl?sid=WKPTLP:landingpage{\&}an=00005768-200503000-00014},
volume = {37},
