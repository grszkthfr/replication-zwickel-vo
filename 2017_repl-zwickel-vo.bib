journal = {Science},
month = {sep},
number = {5843},
pages = {1360--1366},
pmid = {17823346},
title = {{Humans Have Evolved Specialized Skills of Social Cognition: The Cultural Intelligence Hypothesis}},
url = {http://www.sciencemag.org/cgi/content/full/317/5843/1360 http://www.sciencemag.org/cgi/content/full/317/5843/1360/DC1 http://www.sciencemag.org/cgi/content/full/317/5843/1360{\#}otherarticles http://www.sciencemag.org/cgi/collection/psychology http://www.sci},
