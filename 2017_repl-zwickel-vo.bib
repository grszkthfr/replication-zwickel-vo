publisher = {Taylor {\&} Francis Group},
title = {{The effects of self-involvement on attention, arousal, and facial expression during social interaction with virtual others: a psychophysiological study}},
url = {http://www.tandfonline.com/doi/abs/10.1080/17470910600985621 http://www.ncbi.nlm.nih.gov/pubmed/18633787},
