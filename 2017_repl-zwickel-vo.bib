pages = {1105--22},
pmid = {19686008},
title = {{Deficits in cross-race face learning: insights from eye movements and pupillometry.}},
url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2871406/pdf/nihms194100.pdf http://www.ncbi.nlm.nih.gov/pubmed/19686008{\%}5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2871406},
