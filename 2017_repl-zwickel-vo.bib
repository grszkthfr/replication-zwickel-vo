@Article{AlMaskati1989,
  author   = {{Al Maskati}, H. a. and Zbrozyna, a. W.},
  title    = {{Stimulation in prefrontal cortex area inhibits cardiovascular and motor components of the defence reaction in rats}},
  journal  = {Journal of the Autonomic Nervous System},
