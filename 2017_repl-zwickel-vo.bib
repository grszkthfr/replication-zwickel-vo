title = {{Tools, Toys, Truisms, and Theories: Some Thoughts on the Creative Cycle of Theory Formation}},
volume = {8},
year = {2004}
}
@article{Watkins1998,
abstract = {OBJECTIVE: Prospective studies have demonstrated that anxiety predicts sudden cardiac death, but the mechanism underlying this increased risk is unclear. This study examined whether anxiety is associated with reductions in vagal control of heart rate in healthy volunteers. METHOD: Trait anxiety (T-ANX) was measured, using the Spielberger State-Trait Anxiety Inventory (STAI), in 93 healthy men and women 25 to 44 years of age. Power spectral analysis was used to measure two indices of vagal control: baroreflex control of heart rate (BRC) and respiratory sinus arrhythmia (RSA). RESULTS: High trait anxiety (T-ANX {\textgreater} 41, N = 23) was associated with significantly reduced vagal control of the heart, compared with low trait anxiety (T-ANX {\textless} 31, N = 22), as indicated by a 36{\%} reduction in BRC (p{\textless} .001) and an 8{\%} reduction in RSA (p{\textless}.05). Furthermore, T-ANX scores were negatively correlated with levels of BRC (r = -.30, p{\textless}.005), and levels of RSA (r = -.26, p {\textless}.05). CONCLUSIONS: These findings provide evidence that trait anxiety is associated with reductions in vagal control of the heart. Additional studies are needed to examine whether low vagal control is involved in the increased risk of sudden cardiac death associated with anxiety.},
author = {Watkins, Lana L and Grossman, Paul and Krishnan, Ranga and Sherwood, Andrew},
file = {:Watkins et al._1998.pdf:pdf},
isbn = {0033-3174 (Print)},
issn = {0033-3174},
journal = {Psychosomatic medicine},
pages = {498--502},
pmid = {9710297},
title = {{Anxiety and vagal control of heart rate.}},
volume = {60},
year = {1998}
}

